@using Web.ViewModels

@model IEnumerable<EtudiantViewModel>

@{
	ViewData["Title"] = "Liste des étudiants";
}

<h1>@ViewData["Title"]</h1>

<p>
	<a asp-action="Add">Ajouter</a>
</p>
<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Email)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.UniversiteId)
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.Email)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.UniversiteId)
				</td>
			</tr>
		}
	</tbody>
</table>
